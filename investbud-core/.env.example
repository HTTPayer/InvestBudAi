# FRED API Key (get from https://fred.stlouisfed.org/docs/api/api_key.html)
FRED_API_KEY=your_fred_api_key_here

# Alchemy API Key for wallet analysis (get from https://www.alchemy.com/)
ALCHEMY_API_KEY=your_alchemy_api_key_here

# Optional: CoinGecko API key for enhanced rate limits
COINGECKO_API_KEY=your_coingecko_api_key_here

# X402 Payment Configuration
PAYTO_ADDRESS=your_ethereum_address_here
PAYMENT_NETWORK=base-sepolia
MAINNET=false

# API Pricing (in USD)
ADVISE_COST=0.10
REGIME_COST=0.01
PORTFOLIO_COST=0.05
CHAT_COST=0.02

# Optional: Custom facilitator URL
FACILITATOR_URL=

# LLM Advisory API Key (httpayer)
SERVER_API_KEY=your_httpayer_api_key_here

# PostgreSQL Database (for chat sessions)
# Format: postgresql://username:password@host:port/database
DATABASE_URL=postgresql://localhost/macrocrypto
